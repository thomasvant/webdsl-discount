("0b580cfb3a8623453bb2d54d1bcb06bf3c964977",["./web.xml"],XmlFile(["."],"web.xml","<?xml version=\"1.0\" ?>\n<web-app version=\"2.4\"\n    xmlns=\"http://java.sun.com/xml/ns/j2ee\"\n    xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\n    xsi:schemaLocation=\"http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd\">\n\n  <display-name>Discount</display-name>\n\n  <description>Discount</description>\n\n  <welcome-file-list>\n      <welcome-file>root</welcome-file>\n  </welcome-file-list>\n  <listener>\n      <display-name>ImageIO service provider loader/unloader</display-name>\n      <listener-class>com.twelvemonkeys.servlet.image.IIOProviderContextListener</listener-class>\n  </listener>\n  <servlet>\n    <servlet-name>jcaptcha</servlet-name>\n    <servlet-class>utils.ImageCaptchaServlet</servlet-class>\n    <load-on-startup>0</load-on-startup>\n  </servlet>\n  <servlet-mapping>\n    <servlet-name>jcaptcha</servlet-name>\n    <url-pattern>/jcaptcha</url-pattern>\n  </servlet-mapping>\n\n  <servlet-mapping>\n    <servlet-name>default</servlet-name>\n    <url-pattern>/robots.txt</url-pattern>\n  </servlet-mapping>\n  <servlet-mapping>\n    <servlet-name>default</servlet-name>\n    <url-pattern>/favicon.ico</url-pattern>\n  </servlet-mapping>\n  <servlet-mapping>\n    <servlet-name>default</servlet-name>\n    <url-pattern>/images/*</url-pattern>\n  </servlet-mapping>\n  <servlet-mapping>\n    <servlet-name>default</servlet-name>\n    <url-pattern>/javascript/*</url-pattern>\n  </servlet-mapping>\n  <servlet-mapping>\n    <servlet-name>default</servlet-name>\n    <url-pattern>/stylesheets/*</url-pattern>\n  </servlet-mapping>\n\n  <servlet>\n    <servlet-name>mainservlet</servlet-name>\n    <servlet-class>utils.DispatchServlet</servlet-class>\n  </servlet>\n  <servlet-mapping>\n    <servlet-name>mainservlet</servlet-name>\n    <url-pattern>/</url-pattern>\n  </servlet-mapping>\n\n  <error-page>\n    <error-code>404</error-code>\n    <location>/pagenotfound</location>\n  </error-page>\n\n<filter>\n  <filter-name>ExpiresFilter</filter-name>\n  <filter-class>fr.xebia.servlet.filter.ExpiresFilter</filter-class>\n  <init-param>\n    <param-name>ExpiresByType image</param-name>\n    <param-value>access plus 7 days</param-value>\n  </init-param>\n  <init-param>\n    <param-name>ExpiresByType text/css</param-name>\n    <param-value>access plus 7 days</param-value>\n  </init-param>\n  <init-param>\n    <param-name>ExpiresByType application/javascript</param-name>\n    <param-value>access plus 7 days</param-value>\n  </init-param>\n  <!-- backwards compatibility -->\n    <init-param>\n      <param-name>ExpiresByType text/javascript</param-name>\n      <param-value>access plus 7 days</param-value>\n    </init-param>\n</filter>\n<filter-mapping>\n  <filter-name>ExpiresFilter</filter-name>\n  <url-pattern>/stylesheets/*</url-pattern>\n  <dispatcher>REQUEST</dispatcher>\n</filter-mapping>\n<filter-mapping>\n  <filter-name>ExpiresFilter</filter-name>\n  <url-pattern>/javascript/*</url-pattern>\n  <dispatcher>REQUEST</dispatcher>\n</filter-mapping>\n<filter-mapping>\n  <filter-name>ExpiresFilter</filter-name>\n  <url-pattern>/images/*</url-pattern>\n  <dispatcher>REQUEST</dispatcher>\n</filter-mapping>\n<filter-mapping>\n  <filter-name>ExpiresFilter</filter-name>\n  <url-pattern>/favicon.ico</url-pattern>\n  <dispatcher>REQUEST</dispatcher>\n</filter-mapping>\n\n</web-app>\n"))